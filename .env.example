# LLM Configuration
OPENAI_API_KEY=your_openai_api_key_here
TAVILY_API_KEY=your_tavily_api_key_here

# LLM Models
FAST_LLM=gpt-4o-mini
SMART_LLM=gpt-4o
STRATEGIC_LLM=gpt-4o
EMBEDDING_MODEL=text-embedding-3-small

# Redis Configuration
REDIS_URL=redis://redis:6379
REDIS_PASSWORD=
REDIS_DB=0

# FAISS Configuration
FAISS_INDEX_PATH=/app/data/faiss_index
FAISS_INDEX_NAME=documents_index
EMBEDDING_DIMENSION=1536

# Document Configuration
DOC_PATH=/app/data/documents
MAX_DOC_SIZE_MB=50
SUPPORTED_FORMATS=pdf,txt,csv,xlsx,md,pptx,docx

# Search Configuration
MAX_SEARCH_RESULTS_PER_QUERY=10
MAX_TOTAL_SEARCH_RESULTS=50
SEARCH_TIMEOUT_SECONDS=30

# Cache Configuration
CACHE_TTL_SECONDS=3600
CACHE_MAX_SIZE_MB=512

# Application Configuration
LOG_LEVEL=INFO
DEBUG=false
ENVIRONMENT=development

# WebSocket Configuration
WS_HEARTBEAT_INTERVAL=30
WS_CONNECTION_TIMEOUT=300

# Performance Configuration
ASYNC_CONCURRENCY_LIMIT=10
FAISS_SEARCH_BATCH_SIZE=32
REDIS_CONNECTION_POOL_SIZE=10